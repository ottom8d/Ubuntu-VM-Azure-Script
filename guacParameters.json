{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "westus2"
        },
        "networkInterfaceName": {
            "value": "ubuntutest181"
        },
        "networkSecurityGroupName": {
            "value": "UbuntuTest1-nsg"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "default-allow-ssh",
                    "properties": {
                        "priority": 1000,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceApplicationSecurityGroups": [],
                        "destinationApplicationSecurityGroups": [],
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "22"
                    }
                }
            ]
        },
        "subnetName": {
            "value": "default"
        },
        "virtualNetworkName": {
            "value": "HashCrackTest-vnet"
        },
        "addressPrefixes": {
            "value": [
                "10.0.19.0/24"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name": "default",
                    "properties": {
                        "addressPrefix": "10.0.19.0/24"
                    }
                }
            ]
        },
        "publicIpAddressName": {
            "value": "UbuntuTest1-ip"
        },
        "publicIpAddressType": {
            "value": "Dynamic"
        },
        "publicIpAddressSku": {
            "value": "Basic"
        },
        "virtualMachineName": {
            "value": "UbuntuTest1"
        },
        "virtualMachineRG": {
            "value": "HashCrackTest"
        },
        "osDiskType": {
            "value": "Standard_LRS"
        },
        "virtualMachineSize": {
            "value": "Standard_B1s"
        },
        "adminUsername": {
            "value": "HashCrackUser1"
        },
        "adminPassword": {
            "value": "This is a basic passphrase"
        },
        "diagnosticsStorageAccountName": {
            "value": "hashcracktestdiag"
        },
        "diagnosticsStorageAccountId": {
            "value": "Microsoft.Storage/storageAccounts/hashcracktestdiag"
        },
        "diagnosticsStorageAccountType": {
            "value": "Standard_LRS"
        },
        "diagnosticsStorageAccountKind": {
            "value": "Storage"
        }
    }
}
